{
    "name": "@terascope/standard-asset-apis",
    "displayName": "Standard Asset Apis",
    "version": "1.1.0",
    "description": "A common set of tools for data processing",
    "homepage": "https://github.com/terascope/standard-assets",
    "repository": "git@github.com:terascope/standard-assets.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "files": [
        "dist/src/**/*"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "build:watch": "yarn build --watch",
        "test": "ts-scripts test . --",
        "test:debug": "ts-scripts test --debug . --",
        "test:watch": "ts-scripts test --watch . --"
    },
    "dependencies": {
        "@sindresorhus/fnv1a": "~3.1.0",
        "@terascope/utils": "~1.9.3"
    },
    "devDependencies": {
        "@terascope/scripts": "~1.19.4",
        "@types/jest": "~30.0.0",
        "@types/node": "~24.0.10",
        "jest": "~30.0.4",
        "jest-extended": "~6.0.0",
        "jest-fixtures": "~0.6.0",
        "ts-jest": "~29.4.0",
        "typescript": "~5.8.3"
    },
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit"
    }
}
